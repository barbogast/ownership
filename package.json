{
  "name": "ownership",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "cp node_modules/sql.js/dist/sql-wasm.wasm public && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test:unit": "vitest --config src/vitest.unit.config.ts",
    "convert-csv": "(cd data && npx ts-node --esm convertCsv.ts)",
    "convert-csv2": "(cd data && npx ts-node --esm convertCsv2.ts)",
    "analyze-bundle": "npx vite-bundle-visualizer",
    "test:e2e": "yarn playwright test --config e2e/playwright.config.ts",
    "test:e2e-ui": "yarn playwright test --config e2e/playwright.config.ts --ui",
    "test:e2e-codegen": "yarn playwright codegen http://localhost:5173"
  },
  "dependencies": {
    "@blocknote/core": "^0.9.6",
    "@blocknote/react": "^0.9.6",
    "@isomorphic-git/lightning-fs": "^4.6.0",
    "@monaco-editor/react": "^4.6.0",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.9.3",
    "@types/papaparse": "^5.3.12",
    "@types/sql.js": "^1.4.9",
    "@types/uuid": "^9.0.7",
    "antd": "^5.11.2",
    "buffer": "^6.0.3",
    "csv-parse": "^5.5.2",
    "csv-stringify": "^6.4.4",
    "dayjs": "^1.11.10",
    "idb-keyval": "^6.2.1",
    "immer": "^10.0.3",
    "isomorphic-git": "^1.25.0",
    "monaco-editor": "^0.44.0",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-resizable-panels": "^0.0.61",
    "recharts": "^2.10.1",
    "remeda": "^1.29.0",
    "safe-stable-stringify": "^2.4.3",
    "slugify": "^1.6.6",
    "source-map-js": "^1.0.2",
    "sql.js": "^1.8.0",
    "uuid": "^9.0.1",
    "wouter": "^2.12.1",
    "zustand": "^4.4.6"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.16",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.54.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "happy-dom": "^12.10.3",
    "typescript": "^5.3.2",
    "vite": "^4.4.3",
    "vitest": "^0.34.6"
  },
  "packageManager": "yarn@3.6.0"
}
