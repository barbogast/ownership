{
  "name": "ownership",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "cp node_modules/sql.js/dist/sql-wasm.wasm public && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest --config src/vitest.unit.config.ts",
    "convert-csv": "(cd data && npx ts-node --esm convertCsv.ts)",
    "convert-csv2": "(cd data && npx ts-node --esm convertCsv2.ts)",
    "analyze-bundle": "npx vite-bundle-visualizer",
    "e2e": "yarn playwright test --config e2e/playwright.config.ts",
    "e2e-ui": "yarn playwright test --config e2e/playwright.config.ts --ui",
    "e2e-codegen": "yarn playwright codegen http://localhost:5173"
  },
  "dependencies": {
    "@blocknote/core": "^0.8.2",
    "@blocknote/react": "^0.8.2",
    "@isomorphic-git/lightning-fs": "^4.6.0",
    "@monaco-editor/react": "^4.5.1",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.4.2",
    "@types/papaparse": "^5.3.7",
    "@types/sql.js": "^1.4.4",
    "@types/uuid": "^9.0.2",
    "antd": "^5.7.0",
    "buffer": "^6.0.3",
    "csv-parse": "^5.4.0",
    "csv-stringify": "^6.4.0",
    "dayjs": "^1.11.9",
    "immer": "^10.0.2",
    "isomorphic-git": "^1.24.3",
    "monaco-editor": "^0.40.0",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-resizable-panels": "^0.0.53",
    "recharts": "^2.7.2",
    "safe-stable-stringify": "^2.4.3",
    "slugify": "^1.6.6",
    "source-map-js": "^1.0.2",
    "sql.js": "^1.8.0",
    "uuid": "^9.0.0",
    "wouter": "^2.11.0",
    "zustand": "^4.3.9"
  },
  "devDependencies": {
    "@playwright/test": "^1.36.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "eslint": "^8.44.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "happy-dom": "^10.9.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.3",
    "vitest": "^0.33.0"
  },
  "packageManager": "yarn@3.6.0"
}
